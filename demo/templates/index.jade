doctype html
html(lang="zh-CN")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
        meta(name="viewport", content="width=device-width, initial-scale=1.0, minimum-scale=1.0")
        title Faâ™‚Muse
        meta(name="keywords", content="OpenMediaLab,FaMuse")
        meta(name="description", content="Audio process through image which is based on the state-of-the-art technology. ")
        meta(name="author", content="OpenMediaLab")
        link(rel="shortcut icon", type="image/png", href="/static/image/logo.png")
        link(rel="stylesheet", href="/static/css/sky.css")
        script(type="text/javascript", src="/static/js/jquery.min.js")
        script(type="text/javascript", src="/static/js/jquery.ui.widget.js")
        script(type="text/javascript", src="/static/js/jquery.iframe-transport.js")
        script(type="text/javascript", src="/static/js/jquery.fileupload.js")

    body
        div#container
            div#uploader
                p#file-name No file selected
                input#file-upload(type="file", name="file", data-url="/upload")
                input#file-upload-button(type="button" value="Browse")
                button#uploader-start(disabled=true) Upload
                button#uploader-cancel(disabled=true) Cancel
                div#progress
                    div#progress-bar(style="width: 0")
            img#current-image
            select#mode-selector
            audio#audio-player
        script(type="text/javascript", src="/static/js/main.js")
